@model List<WebApplication1.Entity.Empresa>
@{
    ViewData["Title"] = "Index";
}

<h2>Empresas</h2>

<table>

    @foreach(var item in Model)
    {
        <span>@item.Nome</span>
    }

</table>


<button class="btn btn-primary">
    @Html.ActionLink("Cadastrar Empresa", "CadastroEmpresa", "Empresa", htmlAttributes: new { Style = "color:White" })
</button>